<% provide(:title, "施設名変更") %>
<h1>施設名変更</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@facility) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>



      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control' %>

      <%= f.submit "保存する", class: "btn btn-primary" %>

    <% end %>



  </div>
</div>
